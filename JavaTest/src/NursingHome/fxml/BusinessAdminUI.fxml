<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="732.0" prefWidth="1139.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NursingHome.controller.BusinessAdminUIController">
   <children>
      <VBox layoutY="1.0" prefHeight="598.0" prefWidth="936.0">
        <children>
          <MenuBar VBox.vgrow="NEVER">
            <menus>
              <Menu mnemonicParsing="false" text="个人">
                <items>
                  <MenuItem fx:id="personInfoMenu" mnemonicParsing="false" onAction="#personalInfo" text="个人信息" />
                  <MenuItem fx:id="logoutMenu" mnemonicParsing="false" onAction="#logout" text="注销" />
                        <MenuItem mnemonicParsing="false" onAction="#backtoMainMenu" text="返回" />
                  <MenuItem fx:id="quitMenu" mnemonicParsing="false" onAction="#quit" text="退出" />
                </items>
              </Menu>
              <Menu mnemonicParsing="false" text="管理">
                <items>
                  <MenuItem mnemonicParsing="false" text="新增客户" />
                  <MenuItem mnemonicParsing="false" text="Unselect All" />
                </items>
              </Menu>
              <Menu mnemonicParsing="false" text="帮助">
                <items>
                  <MenuItem mnemonicParsing="false" onAction="#aboutInfo" text="关于" />
                </items>
              </Menu>
            </menus>
          </MenuBar>
          <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="372.0" prefWidth="198.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="705.0" prefWidth="169.0">
                     <children>
                        <AnchorPane prefHeight="710.0" prefWidth="169.0">
                           <children>
                              <ImageView fitHeight="103.0" fitWidth="102.0" layoutX="30.0" layoutY="26.0" onMouseReleased="#personInfoImage" pickOnBounds="true" preserveRatio="true" />
                              <Label alignment="CENTER" layoutX="38.0" layoutY="129.0" prefHeight="40.0" prefWidth="85.0" text="姓名">
                                 <font>
                                    <Font size="17.0" />
                                 </font>
                              </Label>
                              <JFXButton layoutX="30.0" layoutY="206.0" onAction="#backtoMainMenu" prefHeight="40.0" prefWidth="102.0" text="返回主界面">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </JFXButton>
                           </children>
                        </AnchorPane>
                     </children>
                  </VBox>
                  <Separator layoutX="165.0" layoutY="3.0" orientation="VERTICAL" prefHeight="702.0" prefWidth="6.0" />
                  <TabPane layoutX="168.0">
                     <tabs>
                        <Tab closable="false" text="业务管理">
                           <content>
                              <TableView fx:id="customerTableView" prefHeight="706.0" prefWidth="973.0">
                                <columns>
                                  <TableColumn fx:id="businessID" prefWidth="75.0" text="编号" />
                                  <TableColumn fx:id="businessName" prefWidth="75.0" text="姓名" />
                                    <TableColumn fx:id="businessAge" prefWidth="75.0" text="年龄" />
                                    <TableColumn fx:id="businessEnterTime" prefWidth="75.0" text="入院时间" />
                                    <TableColumn fx:id="businessRoomID" prefWidth="75.0" text="房号" />
                                    <TableColumn fx:id="businessBedID" prefWidth="75.0" text="床号" />
                                    <TableColumn fx:id="businessPhone" prefWidth="75.0" text="电话" />
                                    <TableColumn fx:id="businessCareWorkerID" prefWidth="75.0" text="护工编号" />
                                    <TableColumn fx:id="businessCareType" prefWidth="75.0" text="护理等级" />
                                    <TableColumn fx:id="businessRelationName" prefWidth="75.0" text="亲属姓名" />
                                    <TableColumn fx:id="businessRelation" prefWidth="75.0" text="亲属关系" />
                                    <TableColumn fx:id="businessRelationPhone" prefWidth="75.0" text="亲属电话" />
                                </columns>
                              </TableView>
                           </content>
                        </Tab>
                        <Tab closable="false" text="床位管理">
                           <content>
                              <TableView fx:id="bedTableView" prefHeight="706.0" prefWidth="973.0">
                                 <columns>
                                    <TableColumn fx:id="bedID" prefWidth="75.0" text="编号" />
                                    <TableColumn fx:id="bedRoomID" prefWidth="75.0" text="所在房间" />
                                    <TableColumn fx:id="bedIsPeople" prefWidth="75.0" text="是否使用" />
                                    <TableColumn fx:id="bedRank" prefWidth="75.0" text="床位等级" />
                                 </columns>
                              </TableView>
                           </content>
                        </Tab>
                     </tabs>
                  </TabPane>
               </children>
          </AnchorPane>
        </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
