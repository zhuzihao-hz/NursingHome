                <Label prefHeight="28.0" prefWidth="76.0" text="医生编号" GridPane.rowIndex="3"/>
                <Label prefHeight="28.0" prefWidth="76.0" text="档案内容" GridPane.rowIndex="5"/>
                <Label prefHeight="28.0" prefWidth="76.0" text="记录日期" GridPane.rowIndex="4"/>
                <Label prefHeight="28.0" prefWidth="66.0" text="档案编号"/>
