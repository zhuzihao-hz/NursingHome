                <TextField fx:id="recordCustomerNameTextField" promptText="请输入客户姓名" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
                <JFXTextArea fx:id="recordContext" prefWidth="355.0" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Label fx:id="recordIdLabel" prefHeight="26.0" prefWidth="353.0" text="档案编号" GridPane.columnIndex="1">
